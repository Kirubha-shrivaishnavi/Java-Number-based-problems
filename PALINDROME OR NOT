PALINDROME OR NOT

import java.util.*;
public class Main{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int a = sc.nextInt();
        int ans=a;
        int sum=0;
        int rev=0;
        while(a>0){
            rev=a%10;
            sum=sum*10+rev;
            a=a/10;
        }
        System.out.println((sum==ans)?"Palindrome":"Not Palindrome");
    }
}
