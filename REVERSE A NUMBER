REVERSE A NUMBER

import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int rev = 0;
        while(a!=0){
            rev = a%10;
            a = a/10;
            System.out.print(rev);
        }
    }
}
